@page
@model HospiEnCasa.App.Fronted.pages.Pacientes.IndexModel
@{
}
<h1>Listado de Pacietes</h1>
<table class ="Table">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Apellidos</th>
            <th scope="col">NumeroTelefono</th>
        </tr>
    </thead>
    </tbody>
        @foreach(var paciente in Model.pacientes)
        {
            <tr>
                <td>@paciente.Nombre</td>
                <td>@paciente.Apellidos</td>
                <td>@paciente.NumeroTelefono</td>
            </tr>
        }
    </tbody>
</table>